<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#F3EEEB" />
            <stroke
                android:width="1dp"
                android:color="#DADEE4" />
            <corners android:radius="8dp" />
        </shape>
    </item>
    <item android:width="30dp" android:height="30dp" android:right="0dp" android:end="0dp" android:gravity="right">
        <bitmap
            android:antialias="true"
            android:filter="true"
            android:gravity="fill"
            android:mipMap="true"
            android:src="@drawable/icon_remove" />
    </item>
</layer-list>
